  "action_codes": {
    "description": "Maps conditions to next actions in the ticket processing pipeline",
    "mappings": {
      "valid_invoice_all_checks_pass": {
        "nextAction": "invoice_processing",
        "description": "Invoice is valid, proceed to automated payment processing"
      },
      "amount_discrepancy_detected": {
        "nextAction": "manual_review",
        "description": "Amount does not match expected range, requires human review"
      },
      "vendor_not_approved": {
        "nextAction": "vendor_approval",
        "description": "Vendor is not in approved vendor list, requires procurement approval"
      },
      "hazardous_materials_present": {
        "nextAction": "invoice_processing",
        "description": "Hazardous materials detected, invoice processing with EHS compliance flag",
        "flags": ["EHS_REVIEW_REQUIRED"]
      },
      "past_due_invoice": {
        "nextAction": "invoice_processing",
        "description": "Invoice is past due, prioritize for expedited payment",
        "flags": ["EXPEDITED_PAYMENT"]
      },
      "duplicate_invoice_suspected": {
        "nextAction": "manual_review",
        "description": "Possible duplicate invoice detected, requires human verification"
      },
      "budget_exceeded": {
        "nextAction": "budget_approval",
        "description": "Invoice amount exceeds department budget threshold, requires manager approval"
      }
    }
  }
